<b><%= link_to 'Main Menu', login1_functions_path%></b><br/>
<b><%= link_to 'Back To Stats', stats1_functions_path%></b><br/><br/>

<%if !@call_log.nil? && !@call_log.empty? %>
<h3>Activity for <%=@name%> on <%=@date.to_formatted_s(:long_ordinal)%></h3>
		<table border="1" class="mystyle">
				<tr class="cell">
					<th><b>Time</b></th>
					<th><b>Type</b></th>
					<th><b>Notes</b></th>
				</tr>	
			<%@call_log.each do |k,cl|
				class5='cell'
				if cl.class5=='over'
					class5='r4'
				end
			
			%>
			    
				<tr class='<%=class5%>'>
					<td><b><%= cl.ts.to_s[11..15]%></b></td>
					<td><b><%= cl.type%></b></td>
					<td><b><%= cl.notes%></b></td>
				</tr>	
			<%end%>
		</table>
<%end%>
