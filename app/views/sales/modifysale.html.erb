        function numtomonth(num){
        	if(num=='0')
        		return 'Jan';
        	else if(num=='1')
        	  	return 'Feb';   
        	else if(num=='2')
        	  	return 'Mar';   
        	else if(num=='3')
        	  	return 'Apr';   
        	else if(num=='4')
        	  	return 'May';   
        	else if(num=='5')
        	  	return 'Jun';   
        	else if(num=='6')
        	  	return 'Jul';   
        	else if(num=='7')
        	  	return 'Aug';   
        	else if(num=='8')
        	  	return 'Sep';   
        	else if(num=='9')
        	  	return 'Oct';   
        	else if(num=='10')
        	  	return 'Nov';   
        	else if(num=='11')
        	  	return 'Dec';
        }

        function monthtonum(month){
        	if(month=='Jan')
        		return '0';
        	else if(month=='Feb')
        	  	return '1';   
        	else if(month=='Mar')
        	  	return '2';   
        	else if(month=='Apr')
        	  	return '3';   
        	else if(month=='May')
        	  	return '4';   
        	else if(month=='Jun')
        	  	return '5';   
        	else if(month=='Jul')
        	  	return '6';   
        	else if(month=='Aug')
        	  	return '7';   
        	else if(month=='Sep')
        	  	return '8';   
        	else if(month=='Oct')
        	  	return '9';   
        	else if(month=='Nov')
        	  	return '10';   
        	else if(month=='Dec')
        	  	return '11';
        }
        
