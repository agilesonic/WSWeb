	  function fhst(){
		  var priceText=$('#create_sale_form_jobprice');
          var price=parseFloat(priceText.val());
          var hstRate=parseFloat(".13");
          var hst=price*hstRate;
          var hstText = $('#create_sale_form_jobhst');
          hstText.val(hst.toFixed(2)); 
	  }

	  function ftotal(){
